{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'fdroid-repo-manager'",
      "cargo": {
        "args": [
          "build",
          "--bin=fdroid-repo-manager",
          "--package=fdroid-repo-manager"
        ],
        "filter": {
          "name": "fdroid-repo-manager",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}",
      "env": {
        "RM_IP": "127.0.0.1",
        "RM_PORT": "8080",
        "RM_REPO_PATH": "/home/quio/GitHub/fdroid-repo-manager/developement/fdroid",
        "RUST_LOG": "DEBUG",
        "ANDROID_HOME": "/opt/android-sdk"
      }
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'fdroid-repo-manager'",
      "cargo": {
        "env": {
          "RUST_LOG": "DEBUG"
        },
        "args": [
          "test",
          "--no-run",
          "--bin=fdroid-repo-manager",
          "--package=fdroid-repo-manager"
        ],
        "filter": {
          "name": "fdroid-repo-manager",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}